{
    "name": "mpyw/unclosure",
    "description": "Closure unwrapper especially suited for Laravel PDO",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "illuminate", "mysql", "pdo", "emulation", "prepared"],
    "autoload": {
